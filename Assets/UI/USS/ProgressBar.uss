.loading-bar {
    width: 500px;
}

.unity-progress-bar__background {
    background-color: rgb(26, 22, 22);
    border-left-color: rgb(126, 96, 57);
    border-right-color: rgb(126, 96, 57);
    border-top-color: rgb(126, 96, 57);
    border-bottom-color: rgb(126, 96, 57);
    border-width: 1px;
}

.unity-progress-bar__progress {
    background-color: rgb(111, 35, 34);
    margin: 1px;
}
